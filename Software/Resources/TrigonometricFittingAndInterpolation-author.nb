Cell[CellGroupData[{

Cell["", "DetailsSection"],

Cell[TextData[{
 "Because of the orthogonality of trigonometric functions, coefficients \
obtained from the discrete Fourier transform give the proper weights for both \
least-squares fitting and interpolation. From [1], if ",
 Cell[BoxData[
  FormBox["m", TraditionalForm]], "InlineMath"],
 " is the order of the trigonometric polynomial and ",
 Cell[BoxData[
  FormBox["n", TraditionalForm]], "InlineMath"],
 " is the number of data points, then"
}], "DetailNotes",
 CellChangeTimes->{{3.7074930939920936`*^9, 3.707493094257292*^9}},
 CellID->21838651],

Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{"p", "(", "t", ")"}], "=", 
   RowBox[{
    RowBox[{
     FractionBox["1", 
      SqrtBox["n"]], 
     StyleBox["[",
      FontSize->14], 
     RowBox[{
      RowBox[{
       RowBox[{"cos", "(", 
        RowBox[{"\[Pi]", " ", "m", " ", "t"}], ")"}], " ", 
       RowBox[{"Re", "(", 
        SubscriptBox["y", 
         RowBox[{
          RowBox[{"n", "/", "2"}], "+", "1"}]], ")"}]}], "+", 
      RowBox[{"Re", "(", 
       SubscriptBox["y", "1"], ")"}]}], 
     StyleBox["]",
      FontSize->14]}], "+", 
    RowBox[{
     FractionBox["2", 
      SqrtBox["n"]], 
     RowBox[{
      StyleBox[
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"k", "=", "1"}], 
        RowBox[{
         RowBox[{"m", "/", "2"}], "-", "1"}]],
       FontSize->16], 
      RowBox[{
       StyleBox["[",
        FontSize->14], 
       RowBox[{
        RowBox[{
         RowBox[{"cos", "(", 
          RowBox[{"2", " ", "\[Pi]", " ", "k", " ", "t"}], ")"}], " ", 
         RowBox[{"Re", "(", 
          SubscriptBox["y", 
           RowBox[{"k", "+", "1"}]], ")"}]}], "-", 
        RowBox[{
         RowBox[{"sin", "(", 
          RowBox[{"2", " ", "\[Pi]", " ", "k", " ", "t"}], ")"}], " ", 
         RowBox[{"Im", "(", 
          SubscriptBox["y", 
           RowBox[{"k", "+", "1"}]], ")"}]}]}], 
       StyleBox["]",
        FontSize->14]}]}]}]}]}], TraditionalForm]], "InlineMath",
 FormatType->"TraditionalForm"]], "DetailNotes",
 CellChangeTimes->{{3.7074831945689125`*^9, 3.707483197619912*^9}},
 CellID->81668455],

Cell[TextData[{
 "is the least-squares fit of the data if ",
 Cell[BoxData[
  FormBox[
   RowBox[{"m", "<", "n"}], TraditionalForm]], "InlineMath"],
 " and the interpolating function if ",
 Cell[BoxData[
  FormBox[
   RowBox[{"m", "=", "n"}], TraditionalForm]], "InlineMath"],
 ". In this equation, ",
 Cell[BoxData[
  FormBox["y", TraditionalForm]], "InlineMath"],
 " is the discrete Fourier transform of the data."
}], "DetailNotes",
 CellID->129033231],

